{
  "name": "faktorio-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --port 9189",
    "start": "wrangler dev",
    "migrate": "tsx migrate.ts",
    "generate": "drizzle-kit generate && pnpm local-migrations-generate",
    "local-migrations-generate": "tsx ./scripts/update-fe-migrations.ts",
    "dup": "drizzle-kit up",
    "kill": "kill -9 $(lsof -t -i:9189)",
    "test": "vitest run",
    "w": "vitest",
    "tsc": "tsc --noEmit",
    "check-env": "tsx scripts/check-env.ts",
    "bump": "npm-check-updates -u"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250620.0",
    "@types/debug": "^4.1.12",
    "drizzle-kit": "^0.31.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.20.3"
  },
  "dependencies": {
    "@google/genai": "^1.5.1",
    "@google/generative-ai": "^0.24.1",
    "@libsql/client": "^0.15.9",
    "@paralleldrive/cuid2": "^2.2.2",
    "@pinojs/json-colorizer": "^4.0.0",
    "@tanstack/react-query": "^5.80.10",
    "@trpc/client": "^11.4.2",
    "@trpc/react-query": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "currency-codes": "^2.2.0",
    "debug": "^4.4.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "0.8.2",
    "faktorio-shared": "workspace:*",
    "node-mailjet": "^6.0.8",
    "zod": "^3.25.67"
  }
}
